{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "Debug": {
      "IncludeScopes": true
    }
  },
  "ConnectionStrings": {
    "MariaDb": "Server=your-server;Database=your-database;User=your-user;Password=your-password;",
    "Redis": "localhost:6379"
  },
  "PathCategories": {
    "GpuBoundPaths": [
      "api/generate",
      "api/chat",
      "api/embeddings",
      "v1/chat/completions",
      "txt2img",
      "img2img",
      "transcribe",
      "transcribe/stream",
      "tts",
      "tts_stream"
    ],
    "SpecificServerPaths": [
      "api/show"
    ]
  }
}
